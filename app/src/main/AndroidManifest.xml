<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yuan">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- &lt;!&ndash;使用安卓测试包&ndash;&gt; -->
        <!-- <uses-library android:name="android.test.runner"/> -->

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".person.PersonMain" />
        <activity android:name=".person.login" />
        <activity android:name=".person.regist" />
        <activity android:name=".person.guanliyuanLogin" />
        <activity android:name=".person.gaunliyuanReg" />
        <activity android:name=".person.gaunliyuanMain" />
        <activity android:name=".person.AddScheme" />
        <activity android:name=".person.ChangeSchem" />
        <activity android:name=".person.EditScheme" />
        <activity android:name=".person.AddXiao" />
        <activity android:name=".person.ExitXiao" />
        <activity android:name=".person.ChangeXiao"></activity>
    </application>

</manifest>